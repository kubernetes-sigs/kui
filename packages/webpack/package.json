    "copy-webpack-plugin": "10.2.4",
    "crypto-browserify": "3.12.0",
    "css-loader": "6.6.0",
    "css-minimizer-webpack-plugin": "3.4.1",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.0",
