{
  "name": "@kui-shell/webpack",
  "version": "12.2.0",
  "description": "Kui support for webpack clients",
  "license": "Apache-2.0",
  "author": "Nick Mitchell",
  "homepage": "https://github.com/IBM/kui#readme",
  "bugs": {
    "url": "https://github.com/IBM/kui/issues/new"
