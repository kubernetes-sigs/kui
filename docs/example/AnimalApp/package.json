{
  "name": "AnimalApp",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "watch:source": "tsc --build . --watch",
    "compile": "npx tsc --build . && npx kui-babel && npx kui-prescan",
    "kill": "npm run kill:proxy; kill $(lsof -t -i:908${PORT_OFFSET-0}) > /dev/null 2> /dev/null || true",
    "watch:electron": "export CLIENT_HOME=$(pwd); npx webpack-dev-server --progress",
    "watch": "npm run kill; if [ ! -d node_modules/@kui-shell/build ]; then mkdir -p node_modules/@kui-shell/build; fi; concurrently -n PROXY,ES6,WEBPACK --kill-others 'npm run proxy' 'npm run watch:source' 'npm run watch:electron $WATCH_ARGS'",
    "proxy": "npx kui-run-proxy",
    "kill:proxy": "kill $(lsof -t -i:808${PORT_OFFSET-1}) > /dev/null 2> /dev/null || true",
    "build": "npm run compile && webpack --mode production",
    "postinstall": "npm run compile",
    "start": "WATCH_ARGS='-open' npm run watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@kui-shell/builder": "^8.4.3",
    "@kui-shell/proxy": "^8.4.3",
    "@types/carbon-components-react": "7.6.15",
    "@types/carbon__icons-react": "10.9.0",
    "@types/node": "12.12.31",
    "@types/react": "16.9.26",
    "@types/react-dom": "16.9.5",
    "concurrently": "5.1.0",
    "css-loader": "3.4.2",
    "html-loader": "1.1.0",
    "html-webpack-plugin": "4.2.0",
    "mini-css-extract-plugin": "0.9.0",
    "sass": "1.26.3",
    "sass-loader": "8.0.2",
    "style-loader": "1.1.3",
    "typescript": "3.8.3",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  },
  "dependencies": {
    "@kui-shell/core": "^8.4.3",
    "@kui-shell/plugin-bash-like": "^8.4.3",
    "@kui-shell/plugin-carbon-themes": "^8.4.3",
    "@kui-shell/plugin-client-common": "^8.4.3",
    "@kui-shell/plugin-example": "file:./plugins/plugin-example",
    "@kui-shell/plugin-proxy-support": "^8.4.3",
    "carbon-components": "10.10.3",
    "carbon-components-react": "7.10.3",
    "carbon-icons": "7.0.7"
  }
}
