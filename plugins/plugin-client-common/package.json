{
  "name": "@kui-shell/plugin-client-common",
  "version": "12.2.0",
  "description": "Kui plugin that offers stylesheets",
  "license": "Apache-2.0",
  "author": "Mengting Yan",
  "homepage": "https://github.com/IBM/kui#readme",
  "bugs": {
    "url": "https://github.com/IBM/kui/issues/new"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IBM/kui.git"
  },
  "keywords": [
    "kui",
    "plugin"
  ],
  "main": "dist/index.js",
  "typedocMain": "src/index.ts",
  "module": "mdist/index.js",
  "types": "mdist/index.d.ts",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.2.1",
    "@mdi/font": "7.1.96",
    "@patternfly/react-charts": "6.94.15",
    "@patternfly/react-core": "4.267.6",
    "@patternfly/react-styles": "4.92.3",
    "@patternfly/react-table": "4.112.6",
    "anser": "2.1.1",
    "ansi-regex": "6.0.1",
    "html-entities": "2.3.3",
    "madwizard": "^4.0.1",
    "monaco-editor": "0.34.1",
    "monaco-editor-webpack-plugin": "7.0.1",
    "react-markdown": "8.0.4",
    "needle": "^3.2.0",
    "rehype-raw": "6.1.1",
    "rehype-slug": "5.1.0",
    "remark-code-frontmatter": "1.0.0",
    "remark-collapse": "0.1.2",
    "remark-directive": "2.0.1",
    "remark-emoji": "3.0.2",
    "remark-frontmatter": "4.0.1",
    "remark-gfm": "3.0.1",
    "spinkit": "2.0.1",
    "string-similarity-coloring": "1.0.12",
    "tmp": "0.2.1",
    "turndown": "7.1.1",
    "turndown-plugin-gfm": "1.0.2",
    "which": "3.0.0"
  },
  "kui": {
    "webpack": {
      "plugins": [
        {
          "plugin": "monaco-editor-webpack-plugin",
          "options": {
            "languages": [
              "css",
              "scss",
              "less",
              "dockerfile",
              "go",
              "html",
              "handlebars",
              "javascript",
              "typescript",
              "json",
              "markdown",
              "python",
              "powershell",
              "shell",
              "yaml"
            ],
            "features": [
              "!anchorSelect",
              "!accessibilityHelp",
              "!bracketMatching",
              "!codelens",
              "!dnd",
              "!iPadShowKeyboard",
              "!multicursor",
              "!quickCommand",
              "!quickOutline",
              "!codeAction",
              "!coreCommands",
              "!fontZoom",
              "!gotoError",
              "!gotoSymbol",
              "!hover",
              "!inlineCompletions",
              "!referenceSearch",
              "!snippets",
              "!suggest",
              "!colorPicker",
              "!toggleHighContrast",
              "!colorDetector"
            ]
          }
        }
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
